{
  "name": "@sampittko/gatsby-starter-intl-blog",
  "private": true,
  "description": "Gatsby starter for creating an internationalized personal blog website.",
  "author": "Samuel Pitoňák <sampittko@gmail.com>",
  "homepage": "https://sampittko.sk",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run watch:css && gatsby develop",
    "develop": "npm run watch:css && gatsby develop",
    "build": "npm run build:css && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "postbuild": "rimraf build/**/*.map",
    "build:css": "postcss src/assets/css/tailwind.css -o src/assets/css/main.css",
    "watch:css": "postcss src/assets/css/tailwind.css -o src/assets/css/main.css",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "dependencies": {
    "@mdx-js/react": "^1.6.16",
    "beautiful-react-hooks": "^0.27.2",
    "gatsby": "^2.24.4",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-intl": "^0.3.3",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-mdx": "^1.2.34",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-scroll-indicator": "^1.0.0",
    "gatsby-plugin-sharp": "^2.6.19",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-remark-copy-linked-files": "^2.3.12",
    "gatsby-remark-images": "^3.3.23",
    "gatsby-remark-smartypants": "^2.3.10",
    "gatsby-source-filesystem": "^2.3.20",
    "gatsby-transformer-sharp": "^2.5.11",
    "react": "^16.3.2",
    "react-cookie-consent": "^5.1.3",
    "react-dom": "^16.3.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.5",
    "lodash": "^4.17.19",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.5.1",
    "tailwindcss-dark-mode": "^1.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sampittko/gatsby-starter-intl-blog"
  },
  "bugs": {
    "url": "https://github.com/sampittko/gatsby-starter-intl-blog/issues"
  }
}
